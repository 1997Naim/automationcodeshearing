<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Adhoc">
<listeners>
<listener class-name="peoplentech.mrsaleemtraining.listners"></listener>
</listeners>
<test name="RegressionofChrome">
<parameter name="browser" value="Chrome"></parameter>
<parameter name="wbsite" value="https://www.facebook.com"></parameter>
<classes>
<class name="myfb.findelement"></class>
<class name="peoplentech.mrsaleemtraining.dataprovider"></class>
</classes>
</test>
<test name="RegressionofEdge">
<parameter name="browser" value="Edge"></parameter>
<parameter name="wbsite" value="https://www.facebook.com"></parameter>
<classes>
<class name="myfb.findelement"></class>
<class name="peoplentech.mrsaleemtraining.dataprovider"></class>
</classes>
</test>
</suite>